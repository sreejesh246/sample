class encap{

	private int id;
	private String name;
	

	int getEmpid(){return id;}
	String getEmpName(){return name;}

	void setEmpid(int newvalue){id = newvalue;}

	void setEmpName(String newvalue){ name = newvalue;}
}


class Encapsulation{

	public static void main (String args[]){
	
	Encapsulation obj = new Encapsulation();
	obj.setEmpid(1);
	obj.setEmpName("dytydyw");

	System.out.println(obj.getEmpid);
	System.out.println(obj.getEmpName);
	}

}
